@model Steller.Models.TblService
@{
    ViewBag.Title = "UpdateService";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hizmet Güncellme Sayfası</h2>
<form method="post">
    @Html. HiddenFor(x=>x. ServiceId)
    <label>Hizmet Ad1</label>
    @Html.TextBoxFor(x => x.servicedName, new { @class = "form-control" })
    <br />
    <label>Hizmet İkonu</label>
    @Html.TextBoxFor(x => x.serviceliconUrl, new { @class = "form-control" })
    <br />
    <button class="btn btn-outline-primary">Güncelle</button>
</form>
