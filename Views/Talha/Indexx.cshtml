@model List<Steller.Models.Talha>
@{
    ViewBag.Title = "Indexx";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Resim</th>
        <th>Ad Soyad</th>
        <th>Açıklama</th>
        <th>Unvan</th>
        <th>Sil</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AboutId</th>
            <td><img src="@item.ImageUrl" style="height:50px;width:50px" /></td>
            <td>@item.[NameSurname]</td>
            <td>@item.Description</td>
            <td>@item.Title</td>

            <td><a href="#" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="#" class="btn btn-outline-info">Güncelle</a></td>

        </tr>
    }


</table>
<br />
<td><a href="#" class="btn btn-outline-primary">Yeni Hakkımızda Bilgisi Ekle</a></td>



